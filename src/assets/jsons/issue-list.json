{
  "pageHeading": "Issue",
  "collectionName": "issue",
  "editMode": "popup",
  "showbutton": true,
  "screenWidth": "40%",
  "actions": [
    {
      "label": "Edit",
      "icon": "edit",
      "formName": "entities/issue"
    },
    {
      "label": "Delete",
      "icon": "delete",
      "formName": "entities/issue"
    }
  ],
  "addRoute": "add/issue",
  "editRoute": "edit/issue",
  "columnDefs": [
    {
      "headerName": "Issue Id",
      "field": "issueid",
      "sortable": true,
      "filter": "agTextColumnFilter"
    },
    {
      "headerName": "Issue Name",
      "field": "issuename",
      "sortable": true,
      "filter": "agTextColumnFilter"
    },
    {
      "headerName": "Project Name",
      "field": "projectname",
      "sortable": true,
      "filter": "agTextColumnFilter"
    },
    {
      "headerName": "Issue Type",
      "field": "issuetype",
      "sortable": true,
      "filter": "agTextColumnFilter"
    },
    {
      "headerName": "Assign To",
      "field": "assignto",
      "sortable": true,
      "filter": "agTextColumnFilter"
    },
    {
      "headerName": "Reporter",
      "field": "reporter",
      "sortable": true,
      "filter": "agTextColumnFilter"
    },
    {
      "headerName": "Remark",
      "field": "remarks",
      "sortable": true,
      "filter": "agTextColumnFilter"
    },
    {
      "headerName": "Status",
      "field": "status",
      "sortable": true,
      "editable": true,
      "filter": "agTextColumnFilter",
      "cellEditor": "agSelectCellEditor",
      "cellEditorParams": {
        "values": [
          "Open",
          "Closed"
        ]
      }
    },
    {
      "type": "rightAligned",
      "headerName": "Actions",
      "cellRenderer": "buttonRenderer",
      "lockPosition": "right",
      "cellRendererParams": {
        "onClick": "this.onBtnClick1.bind(this)",
        "label": "Click 1"
      }
    }
  ]
}